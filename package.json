{
  "name": "material-planner",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "preinstall": "npx only-allow pnpm",
    "postinstall": "pnpm fqlx:generate",
    "dev": "doppler run -- next dev",
    "build": "next build",
    "build:themes": "reshaped theming --output themes",
    "generate:initialState": "ts-node utils/generateInitialStateFromTypedefs.ts",
    "fqlx:generate": "ts-node node_modules/fauna-typed/src/converter.ts",
    "start": "next start",
    "lint": "next lint",
    "vercel:link": "vercel link --yes --token $VERCEL_TOKEN --scope dechea",
    "vercel:pull": "vercel env pull --token $VERCEL_TOKEN --scope dechea"
  },
  "dependencies": {
    "@clerk/nextjs": "4.21.1",
    "@vercel/analytics": "^1.1.0",
    "classnames": "2.3.2",
    "crisp-sdk-web": "^1.0.21",
    "dompurify": "3.0.2",
    "fauna-typed": "0.2.11",
    "formik": "2.4.3",
    "latest": "0.2.0",
    "moment": "2.29.4",
    "next": "13.4.5",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-infinite-scroll-component": "6.1.0",
    "reshaped": "2.4.2",
    "tailwindcss": "3.3.2",
    "yup": "1.2.0",
    "zustand": "4.3.8"
  },
  "devDependencies": {
    "@types/node": "20.2.5",
    "@types/react": "18.2.9",
    "@types/react-dom": "18.2.4",
    "eslint": "8.42.0",
    "eslint-config-next": "13.4.4",
    "path": "0.12.7",
    "prettier": "2.8.8",
    "prettier-plugin-tailwindcss": "0.2.8",
    "ts-node": "10.9.1",
    "typescript": "5.1.3",
    "vercel": "^32.2.2"
  }
}
